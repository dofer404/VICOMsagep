			<![CDATA[SELECT
	t_cor.nombre_tipocor ,
	cor.direccion,
		per.id_persona
	FROM es_sagep.personas per
	JOIN es_sagep.correos cor ON per.id_persona = cor.id_persona
	JOIN es_sagep.tipos_correos t_cor ON cor.id_tipo_correo = t_cor.id_tipo_correo
where per.id_persona = $P{id_persona_dataset}]]>
